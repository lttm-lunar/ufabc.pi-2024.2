      "source": [
        "import math\n",
        "def lim1(x):\n",
        "  if (2 <= x and x < 3):\n",
        "    return math.pow((x - 1),2) + 1\n",
        "  if (3 <= x and x <= 7):\n",
        "    return  1 - math.pow((x - 4),2) - 2\n",
        "  if (x < 2 or x > 7):\n",
        "    return 2\n",
        "\n",
        "x = float(input())\n",
        "print(\"{:.4f}\".format(lim1(x)))\n"
      ]
